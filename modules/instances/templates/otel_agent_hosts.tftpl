[otel_agent_hosts:children]
mysql_servers
ubuntu_servers
rocky_servers
windows_servers

[mysql_servers]
%{ for index, ip in mysql_servers ~}
${prefix}-mysql-${index+1} ansible_host=${ip}
%{ endfor ~}

[mysql_servers:vars]
ansible_become=true
ansible_become_method=sudo

[ubuntu_servers]
%{ for index, ip in ubuntu_servers ~}
${prefix}-ubuntu-${index+1} ansible_host=${ip}
%{ endfor ~}

[ubuntu_servers:vars]
ansible_become=true
ansible_become_method=sudo

[rocky_servers]
%{ for index, ip in rocky_servers ~}
${prefix}-rocky-${index+1} ansible_host=${ip}
%{ endfor ~}

[rocky_servers:vars]
ansible_become=true
ansible_become_method=sudo
ansible_user=rocky

[windows_servers]
%{ for index, ip in windows_servers ~}
${prefix}-windows-${index+1} ansible_host=${ip}
%{ endfor ~}

[windows_servers:vars]
ansible_user=Administrator
ansible_password=${windows_server_administrator_pwd}
ansible_connection=winrm
ansible_winrm_server_cert_validation=ignore
ansible_winrm_transport=ntlm
ansible_port=5985

[all:vars]
access_token=${access_token}
realm=${realm}